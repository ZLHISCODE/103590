Attribute VB_Name = "DayCalendar"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = True
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Option Explicit
'缺省属性值:
Const m_def_SkipX As Integer = 50
Const m_def_SkipY As Integer = 50
Const m_def_AlignmentX = 0
Const m_def_AlignmentY = 0
Const m_def_ForeColor = 0
Const m_def_Enabled = 0
Const m_def_MultiSelectionMode As Boolean = False
Const m_def_BorderStyle = True
'属性变量:
Dim m_SkipX As Integer
Dim m_SkipY As Integer
Dim m_AlignmentX As gWeeksAlignmentX
Dim m_AlignmentY As gWeeksAlignmentY
Dim m_ForeColor As Long
Dim m_Enabled As Boolean
Dim m_Font As Font
Dim m_MultiSelectionMode As Boolean
Dim m_BorderStyle As Boolean

'事件声明:
Event Click(idxDay As Integer, Value As Boolean, Text As String)

Private Sub UserControl_Resize()
    Dim sngTop As Single, intSkip As Integer
    Dim sngLeft As Single, i As Long
    Dim sngStepW As Single, sngStepH As Single
    Dim sngLeftTemp As Single
    
    Err = 0: On Error Resume Next
    sngLeft = ScaleLeft + 50: sngTop = ScaleTop + 50
    sngStepW = (ScaleWidth - btnDay(0).Width * 7 - 100) \ 2
    If sngStepW < 0 Then sngStepW = 50
    sngStepH = (ScaleHeight - btnDay(0).Height * 5 - 100) \ 2
    If sngStepH < 0 Then sngStepH = 50
    Select Case m_AlignmentX
    Case Weeks_AutoSizeWidth
    Case Weeks_Left_Justify
        sngStepW = IIf(m_SkipX <= 0, 100, m_SkipX)
    Case Weeks_CenterX_Justify
        sngStepW = IIf(m_SkipX <= 0, 100, m_SkipX)
        sngLeft = (ScaleWidth - btnDay(0).Width * 7 - sngStepW * 2 - sngLeft * 2) \ 2
    Case Weeks_Right_Justify
        sngStepW = IIf(m_SkipX <= 0, 100, m_SkipX)
        sngLeft = ScaleWidth - btnDay(0).Width * 7 - sngStepW * 2 - sngLeft
    End Select
    sngStepW = 0
    Select Case m_AlignmentY
    Case Weeks_AutoSizeWidth '
    Case Weeks_Top_Justify
        sngStepH = IIf(m_SkipY <= 0, 100, m_SkipY)
    Case Weeks_CenterY_Justify
        sngStepH = IIf(m_SkipY <= 0, 100, m_SkipY)
        sngTop = (ScaleHeight - btnDay(0).Height * 5 - sngStepH * 2 - sngTop * 2) \ 2
    Case Weeks_Bottom_Justify
        sngStepH = IIf(m_SkipY <= 0, 100, m_SkipY)
        sngTop = ScaleHeight - btnDay(0).Height * 5 - sngStepH * 2 - sngTop
    End Select
    sngLeftTemp = sngLeft
    For i = 0 To btnDay.UBound
        If i Mod 7 = 0 Then
            sngLeftTemp = sngLeft
            If i <> 0 Then sngTop = sngTop + btnDay(i).Height + sngStepH
        Else
            sngLeftTemp = sngLeftTemp + btnDay(i).Width + sngStepW
        End If
        btnDay(i).Left = sngLeftTemp
        btnDay(i).Top = sngTop
    Next
    With shpLine
        .Left = ScaleLeft
        .Width = ScaleWidth
        .Top = ScaleTop
