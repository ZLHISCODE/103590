Private Sub btnDay_Click(Index As Integer)
    Dim i As Integer, blnSelected As Boolean
    
    If MultiSelectionMode = False Then '单选模式
        blnSelected = btnDay(Index).Value
        For i = btnDay.LBound To btnDay.UBound
            btnDay(i).Value = False
        Next
        btnDay(Index).Value = blnSelected
    End If
    RaiseEvent Click(Index, btnDay(Index).Value, btnDay(Index).Caption)
End Sub

