'为用户控件初始化属性
Private Sub UserControl_InitProperties()
    m_ForeColor = m_def_ForeColor
    m_Enabled = m_def_Enabled
    Set m_Font = Ambient.Font
    m_AlignmentX = m_def_AlignmentX
    m_AlignmentY = m_def_AlignmentY
    m_SkipX = m_def_SkipX
    m_SkipY = m_def_SkipY
    m_MultiSelectionMode = m_def_MultiSelectionMode
    m_BorderStyle = m_def_BorderStyle
    Call UserControl_Resize
End Sub

'从存贮器中加载属性值
Private Sub UserControl_ReadProperties(PropBag As PropertyBag)

    UserControl.BackColor = PropBag.ReadProperty("BackColor", &H8000000F)
    m_ForeColor = PropBag.ReadProperty("ForeColor", m_def_ForeColor)
    m_Enabled = PropBag.ReadProperty("Enabled", m_def_Enabled)
    Set m_Font = PropBag.ReadProperty("Font", Ambient.Font)
    shpLine.BackStyle = PropBag.ReadProperty("BackStyle", 0)
    shpLine.BorderColor = PropBag.ReadProperty("BorderColor", -2147483645)
    m_AlignmentX = PropBag.ReadProperty("AlignmentX", m_def_AlignmentX)
    m_AlignmentY = PropBag.ReadProperty("AlignmentY", m_def_AlignmentY)
    m_SkipX = PropBag.ReadProperty("SkipX", m_def_SkipX)
    m_SkipY = PropBag.ReadProperty("SkipY", m_def_SkipY)
    m_MultiSelectionMode = PropBag.ReadProperty("MultiSelectionMode", m_def_MultiSelectionMode)
    m_BorderStyle = PropBag.ReadProperty("BorderStyle", m_def_BorderStyle)
    
    shpLine.Visible = m_BorderStyle
    Call UserControl_Resize
End Sub

'将属性值写到存储器
Private Sub UserControl_WriteProperties(PropBag As PropertyBag)
    Call PropBag.WriteProperty("BackColor", UserControl.BackColor, &H8000000F)
    Call PropBag.WriteProperty("ForeColor", m_ForeColor, m_def_ForeColor)
    Call PropBag.WriteProperty("Enabled", m_Enabled, m_def_Enabled)
    Call PropBag.WriteProperty("Font", m_Font, Ambient.Font)
    Call PropBag.WriteProperty("BackStyle", shpLine.BackStyle, 0)
    Call PropBag.WriteProperty("BorderColor", shpLine.BorderColor, -2147483645)
    Call PropBag.WriteProperty("AlignmentX", m_AlignmentX, m_def_AlignmentX)
    Call PropBag.WriteProperty("AlignmentY", m_AlignmentY, m_def_AlignmentY)
    Call PropBag.WriteProperty("SkipX", m_SkipX, m_def_SkipX)
    Call PropBag.WriteProperty("SkipY", m_SkipY, m_def_SkipY)
    Call PropBag.WriteProperty("MultiSelectionMode", m_MultiSelectionMode, m_def_MultiSelectionMode)
    Call PropBag.WriteProperty("BorderStyle", m_BorderStyle, m_def_BorderStyle)
End Sub

